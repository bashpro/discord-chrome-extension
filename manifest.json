{
  "manifest_version": 3,
  "name": "Midjourney prompt",
  "description": "discord midjourney promtp helper",
  "version": "1.0",
  "content_scripts": [
    {
      "js": [
        "https://github.com/bashpro/discord-chrome-extension/blob/main/public/build/bundle.js"
      ],
      "css":[
        "public/build/bundle.css"
      ],
      "run_at": "document_end",
      "matches": [
        "https://discord.com/*"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "permissions": [
    "storage",
    "activeTab"
  ]
}